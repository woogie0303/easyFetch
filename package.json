{
  "name": "@woogie0303/easyfetch",
  "version": "0.1.3",
  "description": "",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      }
    }
  },
  "scripts": {
    "prepare": "husky && husky install",
    "type-check": "tsc -p tsconfig.json --noEmit",
